<!-- Responsive panel example-->

<div class="row">
  <div class="col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Gym2 #<%= @gym2.id %></h3>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-12">
            <dl class="dl-horizontal">
              <dt>Name</dt>
              <dd><%= @gym2.name %></dd>

              <dt>Address</dt>
              <dd><%= @gym2.address %></dd>

              <dt>City</dt>
              <dd><%= @gym2.city %></dd>

              <dt>State</dt>
              <dd><%= @gym2.state %></dd>

              <dt>Zip</dt>
              <dd><%= @gym2.zip %></dd>

              <dt>Phone</dt>
              <dd><%= @gym2.phone %></dd>

              <dt>Website</dt>
              <dd><%= @gym2.website %></dd>

              <dt>Weights</dt>
              <dd><%= @gym2.weights %></dd>

              <dt>Cardio</dt>
              <dd><%= @gym2.cardio %></dd>

              <dt>Pool</dt>
              <dd><%= @gym2.pool %></dd>

            </dl>
            <div class="btn-group btn-group-justified">
              <%= link_to gym2s_url, :class => "btn btn-primary" do %>
                <i class="fa fa-chevron-left"></i>
              <% end %>
              <%= link_to edit_gym2_url(@gym2.id), :class => "btn btn-warning" do %>
                <i class="fa fa-edit"></i>
              <% end %>
              <%= link_to gym2_url(@gym2.id), :method => "delete", :data => { :confirm => "Do you really want to delete this gym2?" }, :class => "btn btn-danger" do %>
                <i class="fa fa-trash-o"></i>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- End responsive panel example -->
